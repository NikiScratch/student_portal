        <td><%= link_to subject.name, addsubject_schedules_path(:schedule =>@schedule.id, :subject_id => subject.id) %></td>
    </tr>
    <% end %>
</table>  


<%= simple_form_for @schedule do |f| %>
 
    <%= f.association :subjects, :include_blank => false %>
  
